    "test": "npm run build && nyc mocha dist/test/*.test.js --exit --timeout=20000",
    "lint": "./node_modules/.bin/eslint src --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines":{
